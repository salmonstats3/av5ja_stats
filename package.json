{
  "name": "salmonstats-app-vite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "VITE_APP_GIT_COMMIT_HASH=$(git rev-parse HEAD) VITE_APP_GIT_COMMIT_DATE=$(git show -s --format=%ci HEAD) vite build",
    "generate": "ts-node src/index.ts",
    "dev": "VITE_APP_GIT_COMMIT_HASH=$(git rev-parse HEAD) VITE_APP_GIT_COMMIT_DATE=$(git show -s --format=%ci HEAD) vite",
    "deploy": "yarn wrangler pages publish dist",
    "predeploy": "yarn build",
    "preview": "vite preview",
    "lint": "eslint './src/**/*.{js,ts,vue}' --fix",
    "format": "prettier --write --ignore-path .gitignore './src/**/*.{js,ts,vue}'"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@types/big.js": "^6.1.6",
    "big.js": "^6.2.1",
    "chart.js": "^4.2.1",
    "class-transformer": "^0.5.1",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "reflect-metadata": "^0.1.13",
    "roboto-fontface": "*",
    "vue": "^3.2.47",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuetify": "npm:@vuetify/nightly@next",
    "webfontloader": "^1.0.0",
    "wrangler": "^2.18.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@types/node": "^20.5.1",
    "@types/webfontloader": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "axios": "^1.4.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^1.4.2",
    "yaml-loader": "^0.8.0"
  },
  "type": "module"
}
